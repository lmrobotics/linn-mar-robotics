<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android1:id="@+id/GridLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bc"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android1:columnCount="15"
    tools:context=".MainActivity" >

    <CheckBox
        android1:id="@+id/Mobility_Bonus"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="40dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Mobility Bonus"
        tools:ignore="HardcodedText" />

    <CheckBox
        android1:id="@+id/Hot_Goals"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="70dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Hot Goals" />

    <CheckBox
        android1:id="@+id/Auto_Bonus"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="100dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Autonomous Bonus" />

    <CheckBox
        android1:id="@+id/Foul"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="130dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Foul" />

    <CheckBox
        android1:id="@+id/Tech_Foul"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="190dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Technical Foul" />

    <CheckBox
        android1:id="@+id/Mechanical_Problems"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="160dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Mechanical Problems" />

    <CheckBox
        android1:id="@+id/Blocked_Shots"
        android1:layout_column="0"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="220dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Blocked Shots" />

    <EditText
        android1:id="@+id/Comments_Auto"
        android1:layout_column="0"
        android1:layout_gravity="left|bottom"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:ems="10"
        android1:hint="Enter your comments" >

        <requestFocus />
    </EditText>

    <TextView
        android1:id="@+id/Comments"
        android1:layout_column="0"
        android1:layout_gravity="left|bottom"
        android1:layout_marginBottom="50dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Comments on Auto"
        android1:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android1:id="@+id/Tele_op"
        android1:layout_column="2"
        android1:layout_gravity="left|bottom"
        android1:layout_marginTop="0dp"
        android1:layout_row="0"
        android1:text="Tele-op"
        android1:textAppearance="?android:attr/textAppearanceLarge" />

    <CheckBox
        android1:id="@+id/Truss"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="10dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Truss" />

    <CheckBox
        android1:id="@+id/two_assist"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="40dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="2 Assist" />

    <CheckBox
        android1:id="@+id/three_assist"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="70dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="3 Assist" />

    <CheckBox
        android1:id="@+id/Mobility_Bonus_T"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="100dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Mobility Bonus" />

    <EditText
        android1:id="@+id/Comments_Tele"
        android1:layout_column="2"
        android1:layout_gravity="left|bottom"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:ems="10"
        android1:enabled="true"
        android1:hint="Enter your comments" >

        <requestFocus />
    </EditText>

    <TextView
        android1:id="@+id/Comments_T"
        android1:layout_column="2"
        android1:layout_gravity="left|bottom"
        android1:layout_marginBottom="50dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:text="Comments on Tele"
        android1:textAppearance="?android:attr/textAppearanceLarge" />

    <NumberPicker
        android1:id="@+id/High_Goal"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginLeft="100dp"
        android1:layout_marginTop="20dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2"
        android1:orientation="vertical" />

    <TextView
        android1:id="@+id/Low_Goal_Lable"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginLeft="0dp"
        android1:layout_row="2"
        android1:text="Low Goals" />

    <TextView
        android1:id="@+id/High_Goal_Lable"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginLeft="100dp"
        android1:layout_row="2"
        android1:text="High Goals" />

    <TextView
        android1:id="@+id/Catches_Lable"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginLeft="200dp"
        android1:layout_row="2"
        android1:text="Catches" />

    <NumberPicker
        android1:id="@+id/Catches"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_column="4"
        android1:layout_gravity="right|top"
        android1:layout_marginLeft="200dp"
        android1:layout_marginTop="20dp"
        android1:layout_row="2"
        android1:layout_rowSpan="2" />

    <NumberPicker
        android1:id="@+id/Low_Goal"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginLeft="0dp"
        android1:layout_marginTop="20dp"
        android1:layout_row="2" />

    <TextView
        android1:id="@+id/Autonomous"
        android1:layout_column="0"
        android1:layout_gravity="left|bottom"
        android1:layout_marginTop="0dp"
        android1:layout_row="0"
        android1:text="Autonomous"
        android1:textAppearance="?android:attr/textAppearanceLarge"
        tools:ignore="HardcodedText" />

    <Button
        android1:id="@+id/SentA3"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="8"
        android1:layout_gravity="left|bottom"
        android1:layout_row="2"
        android1:text="Sent A3" />

    <Button
        android1:id="@+id/SentA1"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="8"
        android1:layout_gravity="left|top"
        android1:layout_row="2"
        android1:text="Sent A1" />

    <Button
        android1:id="@+id/SentA2"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="8"
        android1:layout_gravity="left|center_vertical"
        android1:layout_row="2"
        android1:text="Sent A2" />

    <Button
        android1:id="@+id/RecA1"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="6"
        android1:layout_gravity="left|top"
        android1:layout_row="2"
        android1:text="Rec A1" />

    <Button
        android1:id="@+id/RecA3"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="6"
        android1:layout_gravity="left|bottom"
        android1:layout_row="2"
        android1:text="Rec A3" />

    <Button
        android1:id="@+id/RecA2"
        style="?android:attr/buttonStyleSmall"
        android1:layout_column="6"
        android1:layout_gravity="left|center_vertical"
        android1:layout_row="2"
        android1:text="Rec A2" />

    <EditText
        android1:id="@+id/TeamNo"
        android1:layout_width="137dp"
        android1:layout_column="4"
        android1:layout_gravity="left|bottom"
        android1:layout_row="0"
        android1:ems="10"
        android1:enabled="true"
        android1:hint="Team Number"
        android1:inputType="number" />

    <CheckBox
        android1:id="@+id/Mechanical_Problems_T"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="160dp"
        android1:layout_row="2"
        android1:text="Mechanical Problems" />

    <CheckBox
        android1:id="@+id/Tech_Foul_T"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="130dp"
        android1:layout_row="2"
        android1:text="Technical Foul" />

    <CheckBox
        android1:id="@+id/Blocked_Shots_T"
        android1:layout_column="2"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="190dp"
        android1:layout_row="2"
        android1:text="Blocked Shots" />

    <CheckBox
        android1:id="@+id/dAuto"
        android1:layout_column="0"
        android1:layout_gravity="top"
        android1:layout_marginTop="10dp"
        android1:layout_row="2"
        android1:text="Double Autonomous"
        tools:ignore="HardcodedText" />

    <EditText
        android1:id="@+id/editText1"
        android1:layout_width="120dp"
        android1:layout_column="4"
        android1:layout_gravity="right|bottom"
        android1:layout_row="0"
        android1:ems="10"
        android1:hint="MatchID"
        android1:inputType="number" />

    <TextView
        android1:id="@+id/textView1"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_row="3"
        android1:text="Rate this team"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <Spinner
        android1:id="@+id/spinner1"
        android1:layout_width="120dp"
        android1:layout_height="wrap_content"
        android1:layout_column="4"
        android1:layout_gravity="left|top"
        android1:layout_marginTop="20dp"
        android1:layout_row="3"
        android1:entries="@array/spinchoice" />

    <Button
        android1:id="@+id/senddata"
        android1:layout_column="11"
        android1:layout_gravity="center_horizontal|bottom"
        android1:layout_row="3"
        android1:onClick="sendMessage"
        android1:text="Send Data" />

    <CheckBox
        android1:id="@+id/Foul_T"
        android1:layout_column="2"
        android1:layout_gravity="left|bottom"
        android1:layout_marginTop="220dp"
        android1:layout_row="2"
        android1:text="Foul" />

</GridLayout>